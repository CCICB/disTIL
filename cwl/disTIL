    source: tmb/variants
  - id: tmb
    source: tmb/tmb
  - id: ipass
    source: ipass_patient/ipass_score_file
  - id: epic_deconv
    source: epic_and_quantiseq/epic_deconv
  - id: quant_deconv
    source: epic_and_quantiseq/quanitseq_deconv
  run: disTIL.steps/distil_report.cwl
  out:
  - id: disTIL_report
