  - id: hla_json
    source: consensus_hla/consensus_json
  - id: patient_id
    source: patient_id
  - id: pvacseq_i
    source: pvacseq_vcf_prep/mhc_i_filtered_epitopes
  - id: pvacseq_ii
    source: pvacseq_vcf_prep/mhc_ii_filtered_epitopes
